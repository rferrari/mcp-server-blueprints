{
  "name": "@your-org/mcp-server-blueprints",
  "mcpName": "com.your-org/blueprints-mcp",
  "version": "0.1.0",
  "description": "MCP server for interacting with Blueprints backend",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/blueprints-mcp.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "mcp-server-blueprints": "./dist/transports/http.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./platform": {
      "types": "./dist/platform/index.d.ts",
      "import": "./dist/platform/index.js",
      "default": "./dist/platform/index.cjs"
    }
  },
  "dependencies": {
    "@mjackson/multipart-parser": "^0.10.1",
    "common-tags": "^1.8.2",
    "gqlmin": "^0.3.1",
    "graphql": "^16.11.0",
    "openapi-fetch": "^0.13.5",
    "@supabase/mcp-utils": "^0.3.2",
    "zod": "^3.25.0",
    "node-fetch": "^3.3.2"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^22.8.6",
    "@types/node-fetch": "^2.6.12",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.9"
  },
  "scripts": {
    "build": "tsup --clean",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prebuild": "pnpm typecheck",
    "test": "vitest"
  }
}